	"tokenColors": [
		{
			//keywords
			"scope": [
				"keyword.type",
				"keyword.other.using",
				"keyword.other.import.java",
				"keyword.other.directive.using",
				"keyword.other.namespace",
				"keyword.other.class",
				"keyword.other.await",
				"keyword.other.var",
				"keyword.other.new",
				"keyword.other.this",
				"keyword.other.enum",
				"keyword.other.typeof",
				"keyword.other.where",
				"keyword.control.default",
				"keyword.other.interface",
				"storage.type.namespace.cs",
				"keyword.control.context.using",
				"storage.type.var.cs",

				"meta.function.powershell",
				"keyword.other.DML.sql",
				"keyword.control.import",
				"keyword.control.as",
				"keyword.control.from",
				"keyword.control.export",
				"storage.type.function",
				"storage.type.ts", //const let var
				"storage.type.tsx",
				"storage.type.class",
				"storage.type.interface",
				"keyword.operator.new.ts",
				"variable.language.super.tsx",
				"variable.language.java",
				"variable.language.this",
				"storage.type.js",
				"keyword.control.razor.directive",

				"storage.type.def.groovy",
				"storage.type.primitive.java",
				"keyword.control.new.java",
				"keyword.control.java",
				"storage.type.primitive.array.java",
				"keyword.other.package.java",
				"keyword.other.special-method.dockerfile",

				//pascal
				"storage.type.pascal"

			],
			"settings": {
				"foreground": "#FF0000"
			}
		},
		//keywords modifiers: public static ....
		{
			"scope": [
				"storage.modifier",
				"keyword.control.cshtml.transition"
			],
			"settings": {
				"foreground": "#FF0000",
				"fontStyle": "italic"
			}
		},
		// annotation ....
		{
			"scope": [
				"storage.type.annotation.java",
				"punctuation.definition.annotation.java"
			],
			"settings": {
				"foreground": "#35f555",
				"fontStyle": "italic bold"
			}
		},
		//keywords control: if else switch....
		{
			"scope": [
				"controlKeyword",
				"keyword.control.conditional",
				"keyword.control.switch",
				"keyword.control.case",
				"keyword.control.powershell",
				"keyword.control.shell",
				"keyword.control.then.shell",
				"keyword.control.else.shell"
			],
			"settings": {
				"foreground": "#59afff",
				"fontStyle": "bold"
			}
		},
		//keywords flow control: try catch finally for while...
		{
			"scope": [
				"keyword.control.flow",
				"keyword.control.try",
				"keyword.control.catch",
				"keyword.control.finally",
				"keyword.control.loop",
				"keyword.control.powershell",
				"keyword.control.trycatch",
				"keyword.control.pascal"
			],
			"settings": {
				"foreground": "#FF0080",
			}
		},
		//constants
		{
			"scope": [
				"constant",
				"keyword.other.get.cs",
				"keyword.other.set.cs",
				"constant.language",
				"variable.other.constant",
				"entity.name.variable.enum-member",
				"constant.character.entity.tsx"
			],
			"settings": {
				"foreground": "#FFffff",
			}
		},
		//namespaces
		{
			"scope": [
				"namespace",
				"entity.name.namespace",
				"entity.name.type.namespace",
				"entity.name.type.module",
				"storage.modifier.import.java",
				"storage.modifier.package.java"
			],
			"settings": {
				"foreground": "#FF80BF"
			}
		},
		//class
		{
			"scope": [
				"class",
				"entity.name.type.class",
				"variable.other.object",
				"entity.name.type.tsx",
				"entity.other.inherited-class.java",
				"storage.type.java",
				"storage.type.object.array.java",
				"entity.name.type.pascal"
				
			],
			"settings": {
				"foreground": "#2B91AF",
				"fontStyle": "bold"
			}
		},
		//interface
		{
			"scope": [
				"interface",
				"entity.name.type.interface"],
			"settings": {
				"foreground": "#6CC2D8"
			}
		},
		//struct
		{
			"scope": [
				"struct"
			],
			"settings": {
				"foreground": "#1d6478"
			}
		},
		//enum
		{
			"scope": [				
				"entity.name.type.enum"
			],
			"settings": {
				"foreground": "#80ff00"
			}
		},
		//function parameters
		{
			"scope": [
				"parameter",
				"entity.name.variable.parameter",
				"variable.parameter",
				"meta.scriptblock.powershell",
				"variable.other.jsdoc",
				"meta.group.simple.subexpression.powershell",
				"string.unquoted.argument",
				"string.unquoted.argument.shell",
				"constant.other.option.dash.shell"
			],
			"settings": {
				"foreground": "#fba300",
			}
		},
		//class properites
		{
			"scope": [
				"property",
				"variable.other.property",
				"variable.other.object.property",
				"entity.name.variable.property",
				"variable.other.member.powershell",
				"entity.other.attribute-name"
			],
			"settings": {
				"foreground": "#a2ca00",
				"fontStyle": "bold"
			}
		},
		//class fields
		{
			"scope": [
				"field",
				"entity.name.variable.field",
				"source.sql",
				"constant.other.key.groovy",
				"variable.other.definition.java"
			],
			"settings": {
				"foreground": "#c1a500",
			}
		},
		//readonly fields
		{
			"scope": [
				"variable.other.constant.property" //for java
			],
			"settings": {
				"foreground": "#c1a500",
				"fontStyle": "bold underline"
			}
		},		
		//local variables
		{
			"scope": [
				"entity.name.variable.local",
				"variable.other.readwrite",
				"variable.other.readwrite.powershell",
				"variable.other.pascal",
				"variable.other.assignment.shell",
				"variable.other.normal.shell"
			],
			"settings": {
				"foreground": "#ffbaa6"
			}
		},
		//xml tags, json keys, section
		{
			"scope": [
				"entity.name.tag",
				"support.type.property-name.json",
				"meta.object-literal.key",
				"keyword.other.definition.ini",
				"entity.name.section.inno",

				//for log
				"log.date"
				
			],
			"settings": {
				"foreground": "#569CD6"
			}
		},
		//indetifier, function names....
		{
			"scope": [
				"entity.name.function",
				"text.yaml.jinja",
				"meta.method.groovy",
				"source.groovy",
				"meta.function-call.python",
				"source.inno",
				"source.powershell"
			],
			"settings": {
				"foreground": "#FFFF00",
			}
		},
		//buildin method, utility functions
		{
			"scope": [
				"support.function.powershell",
				"support.function.string.sql",
				"support.type.builtin",
				"support.type.primitive",
				"support.function.builtin.shell"
			],
			"settings": {
				"foreground": "#ffe600",
				"fontStyle": "bold underline"
			}
		},
